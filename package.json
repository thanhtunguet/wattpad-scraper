{
  "name": "wattpad-scraper",
  "version": "1.0.0",
  "repository": "https://github.com/thanhtunguet/wattpad-scraper",
  "author": "thanhtunguet <ht@thanhtunguet.info>",
  "license": "MIT",
  "private": false,
  "main": "./build/main.js",
  "scripts": {
    "main:watch": "NODE_ENV=development webpack --config webpack.main.js --watch",
    "main:build": "NODE_ENV=production  webpack --config webpack.main.js",
    "renderer:serve": "NODE_ENV=development webpack serve --config webpack.renderer.js",
    "renderer:build": "NODE_ENV=production  webpack --config webpack.renderer.js",
    "watch": "run-p main:watch renderer:serve",
    "prebuild": "cp public/index.html build/",
    "build": "run-p main:build renderer:build",
    "start": "NODE_ENV=production electron .",
    "start:dev": "NODE_ENV=development electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:mac": "yarn dist -m",
    "dist:linux": "yarn dist -l",
    "dist:windows": "yarn dist -w"
  },
  "build": {
    "appId": "info.thanhtunguet.wattpad-scraper",
    "productName": "Wattpad Scraper",
    "copyright": "Copyright © 2021 Phạm Thanh Tùng <ht@thanhtunguet.info>",
    "directories": {
      "buildResources": "resources"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*"
    ],
    "extraFiles": [
      "credentials"
    ],
    "publish": null
  },
  "dependencies": {
    "antd": "^4.11.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "cheerio": "^1.0.0-rc.5",
    "codemirror": "^5.59.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1",
    "rxjs": "^6.6.3",
    "socket.io": "^3.1.0",
    "socket.io-client": "^3.1.0",
    "web-streams-polyfill": "^3.0.1",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/bootstrap": "^5.0.5",
    "@types/codemirror": "^0.0.106",
    "@types/express": "^4.17.11",
    "@types/express-http-proxy": "^1.6.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.35",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "css-loader": "^5.0.1",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "javascript-obfuscator": "^2.10.3",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2",
    "webpack-obfuscator": "^3.2.0"
  }
}
